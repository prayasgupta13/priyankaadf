		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cathtutorialadf"
		},
		"ADLSgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSgen2'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"errorlog_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'errorlog'"
		},
		"lsdynamics365_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsdynamics365'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"ADLSgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cathtutorialadls.dfs.core.windows.net"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"lsdynamics365_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "crmreport.srv@agreeya.net"
		}
	},
