						"typeProperties": {
							"dataflow": {
								"referenceName": "dataflow1",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"ADLS": {},
									"sqlsink": {}
								}
							},
							"staging": {},
